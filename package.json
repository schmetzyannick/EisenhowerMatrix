{
  "name": "eisenhowermatrix",
  "version": "0.1.0",
  "description": "Eisenhower matrix to organize your daily tasks",
  "main": "index.js",
  "scripts": {
    "serve:prod": "npm run build:prod && node ./dist/backend/index.js",
    "build:prod": "npm run build:backend:prod && npm run build:frontend:prod",
    "build:backend:prod": "cross-env NODE_ENV=production webpack --env production",
    "build:frontend:prod": "npm install && .\\node_modules\\.bin\\ng build",
    "frontend:ng": "./frontend/ng",
    "frontend:start": "ng serve --port 4201",
    "frontend:build": "ng build",
    "frontend:watch": "ng build --watch --configuration development",
    "frontend:test": "ng test",
    "backend:serve:dev": "npm run backend:build:dev && node --inspect ./dist/backend/index.js",
    "backend:build:dev": "cross-env NODE_ENV=development webpack --env development",
    "lint": "eslint ./backend ./frontend ./shared --ext .ts",
    "lint:fix": "eslint ./backend ./frontend ./shared --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schmetzyannick/EisenhowerMatrix.git"
  },
  "keywords": [
    "eisenhower",
    "matrix",
    "eisenhower",
    "matrix"
  ],
  "author": "Yannick Schmetz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schmetzyannick/EisenhowerMatrix/issues"
  },
  "homepage": "https://github.com/schmetzyannick/EisenhowerMatrix#readme",
  "dependencies": {
    "@angular/animations": "^13.3.1",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "^13.3.1",
    "@angular/compiler": "^13.3.1",
    "@angular/core": "^13.3.1",
    "@angular/forms": "^13.3.1",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "^13.3.1",
    "@angular/platform-browser-dynamic": "^13.3.1",
    "@angular/router": "^13.3.1",
    "base-64": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "bunyan-debug-stream": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "nconf": "^0.11.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "rxjs": "^7.4.0",
    "sequelize": "^6.29.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.1",
    "@angular/cli": "^13.3.1",
    "@angular/compiler-cli": "^13.3.1",
    "@types/base-64": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.8",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.2",
    "@types/nconf": "^0.10.2",
    "@types/node": "^17.0.23",
    "@types/sequelize": "^4.28.11",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-nibble": "^8.0.1",
    "eslint-plugin-jsdoc": "^38.1.5",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "^4.0.0",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "prettier": "^2.6.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
